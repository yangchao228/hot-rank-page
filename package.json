{
  "name": "hot-rank-page",
  "version": "0.1.0",
  "description": "Hot rank service based on DailyHotApi upstream",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest",
    "test:run": "vitest run"
  },
  "engines": {
    "node": ">=20"
  },
  "keywords": [
    "hot-rank",
    "dailyhotapi",
    "hono"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@hono/node-server": "^1.19.6",
    "hono": "^4.10.5",
    "ioredis": "^5.8.1",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@types/node": "^24.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.7"
  }
}
